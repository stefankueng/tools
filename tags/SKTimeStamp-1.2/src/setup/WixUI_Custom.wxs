      <Property Id="WixUI_InstallDirDlg_Back" Value="LicenseAgreementDlg" />
      <Property Id="WixUI_InstallDirDlg_Next" Value="VerifyReadyDlg" />
      <Property Id="WixUI_InstallDirDlg_Browse" Value="BrowseDlg" />

